@model  AnThinhPhat.Entities.Results.ThuTucResult

<div class="col-sm-12">
    <label>Tên thủ tục</label>
    @Html.TextBoxFor(x => Model.TenThuTuc, htmlAttributes: new { @class = "form-control", @readonly = "readonly" })
</div>
<div class="col-sm-12">
    <label>Ngày ban hành</label>
    @Html.DisplayFor(x => Model.NgayBanHanh, "DateTime")
</div>
<div class="col-sm-12">
    <label>Nội dung thủ tục</label>
    @Html.TextAreaFor(x => Model.NoiDung, htmlAttributes: new { @class = "form-control", @readonly = "readonly", rows = 5 })
</div>
<div class="col-sm-12">
    <label>Cơ quan thực hiện</label>
    @Html.TextBoxFor(x => Model.CoQuanInfo.Name, htmlAttributes: new { @class = "form-control", @readonly = "readonly" })
</div>
<div class="col-sm-12">
    <label>Lĩnh vực thủ tục</label>
    @Html.TextBoxFor(x => Model.LinhVucThuTucInfo.Name, htmlAttributes: new { @class = "form-control", @readonly = "readonly" })
</div>

<div class="col-sm-12">
    <label>Danh sách các tập tin</label>
    <br />
    @{ Html.RenderPartial("_PartialPageFiles", Model.Files);}
</div>
