@model string
<div class="modal fade in" id="modalPopupDetail">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">@Model</h4>

            </div>
            <div class="modal-body" id="modalPopupDetailBody">
            </div>
            <div class="modal-footer">
                <div class="col-sm-12">
                    <button type="button" data-dismiss="modal" class="btn btn-default btn-width-md">Đóng</button>
                </div>
            </div>
        </div>
    </div>
</div>
