@model System.Collections.Generic.List<AnThinhPhat.Entities.Infos.CoQuanInfo>

<table class="table table-bordered">
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
            {
            @Html.HiddenFor(x => Model[i].Id)
            <tr>
                <td class="col-sm-10">@Model[i].Name</td>
                <td class="col-sm-2">
                    @Html.CheckBoxFor(x => Model[i].IsSelected)
                </td>
            </tr>
        }
    </tbody>
</table>
