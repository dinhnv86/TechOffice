@model string
@{
    ViewBag.Title = "Index";
}

<link href="~/Content/PagedList.css" rel="stylesheet" type="text/css" />

<div class="row">
    <h2>@Model</h2>

    <input type="button" class="btn btn-link text-right" id="lnkCreateNew" value="Thêm mới" />

    @{Html.RenderPartial("_PartialPageAlert");}

    @Html.Action("List")

    <div class="modal fade in" id="modalCreateNew">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header"></div>
                <div class="modal-body">
                    @{Html.RenderPartial("_PartialPageBaseDataCreate", new AnThinhPhat.ViewModel.BaseDataViewModel());}
                </div>
                @*<div class="modal-footer"></div>*@
            </div>
        </div>
    </div>

    <div class="modal fade in" id="modalEdit">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                </div>
                <div class="modal-body" id="modalEditBody">

                </div>
                @*<div class="modal-footer"></div>*@
            </div>
        </div>
    </div>
</div>
